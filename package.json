    "prepublishOnly": "flow && yarn checkFormat"
  },
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "flow-bin": "0.91.0",
    "prettier": "1.15.3"
  }
}
